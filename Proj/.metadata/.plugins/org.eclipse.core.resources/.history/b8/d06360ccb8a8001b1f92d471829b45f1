package projectoEntrega1;

public class WeightedGraph {
	
	int dim;
	float m[][];
	
	//Cria o grafo como uma matriz, em que se a entrada M_{i,j} for -1 
	//não há aresta a ligar i e j, caso contrário há uma aresta com peso i,j
	public WeightedGraph(int dim) {
		
		super();
		this.dim = dim;
		this.m = new float[dim][dim];
		
		for(int i = 0; i<m.length, i++) {
			for(int j=0, i<m.length, j++)
		}
	}

}
